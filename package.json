{
    "name": "Siba-desktop-app",
    "productName": "Siba-desktop",
    "description": "Sistem Informasi Bengkel Atma Auto",
    "main": "main.js",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "electron .",
        "dev": "npm run development",
        "create-installer-win": "node installer/windows/createinstaller.js",
        "package-win": "electron-packager . Siba-desktop-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=public/AA_Logo.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Siba App\"",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "build": "webpack --mode development",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "devDependencies": {
        "axios": "^0.21",
        "cross-env": "^5.1",
        "electron": "^5.0.2",
        "electron-packager": "^13.1.1",
        "electron-winstaller": "^3.0.4",
        "jquery": "^3.4.0",
        "laravel-mix": "^4.0.7",
        "lodash": "^4.17.5",
        "popper.js": "^1.15.0",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.17.4",
        "sass-loader": "^7.1.0",
        "vue": "^2.5.17",
        "vue-template-compiler": "^2.6.10"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.8.1",
        "apexcharts": "^3.6.10",
        "buefy": "^0.7.3",
        "bulma": "^0.7.4",
        "chart.js": "^2.8.0",
        "hooper": "^0.1.5",
        "laravel-vue-pagination": "^2.3.0",
        "vue-apexcharts": "^1.3.5",
        "vue-axios": "^2.1.4",
        "vue-html-to-paper": "^1.0.3",
        "vue-router": "^3.0.2",
        "vue-simple-spinner": "^1.2.8",
        "vue-swal": "^1.0.0",
        "webpack": "^4.30.0"
    }
}
